    <tr>
      <th>Grade</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @add_fields_to_users.each do |add_fields_to_user| %>
      <tr>
        <td><%= add_fields_to_user.grade %></td>
